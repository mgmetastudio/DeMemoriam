{
  "expo": {
    "name": "Dememoriam",
    "slug": "Dememoriam",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Icons/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/Icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos, so you can upload it on Dememoriam."
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.metastudia.Dememoriamios"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Icons/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.metastudia.Dememoriam"
    },
    "web": {
      "favicon": "./assets/Icons/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5c98a78c-e4e1-49fb-8788-6aa011d68870"
      }
    }
  }
}
